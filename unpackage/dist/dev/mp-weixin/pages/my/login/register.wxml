<view><view class="header"><image style="width:300rpx;height:80rpx;" src="/static/img/logo.png"></image></view><view class="register" style="margin-top:100rpx;padding:0px 10px;"><input type="text" placeholder="账号" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['condition']]]]]}}" value="{{condition.username}}" bindinput="__e"/><input type="text" placeholder="昵称" data-event-opts="{{[['input',[['__set_model',['$0','nickname','$event',[]],['condition']]]]]}}" value="{{condition.nickname}}" bindinput="__e"/><input type="password" password placeholder="密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['condition']]]]]}}" value="{{condition.password}}" bindinput="__e"/></view><button class="bk-background-color " style="border-radius:2.5rem;background-color:#007BFF;width:86%;margin-top:90rpx;" type="primary" data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注册</button></view>